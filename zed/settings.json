{
    "agent": {
        "always_allow_tool_actions": false,
        "default_model": {
            "model": "gemini-2.5-flash",
            "provider": "google"
        }
    },
    "buffer_font_size": 11,
    "features": {
        "edit_prediction_provider": "zed"
    },
    "icon_theme": "Catppuccin Mocha",
    "soft_wrap": "editor_width",
    "telemetry": {
        "diagnostics": true,
        "metrics": true
    },
    "theme": {
        "dark": "Catppuccin Mocha - No Italics",
        "light": "One Light",
        "mode": "system"
    },
    "ui_font_size": 16,
    "vim": {
        "use_system_clipboard": "always"
    },
    "vim_mode": true
}
