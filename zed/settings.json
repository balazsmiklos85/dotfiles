{
  "icon_theme": "Catppuccin Mocha",
  "agent": {
    "always_allow_tool_actions": true,
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-sonnet-4"
    }
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "vim_mode": true,
  "ui_font_size": 16,
  "buffer_font_size": 11,
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "Catppuccin Mocha - No Italics"
  }
}
